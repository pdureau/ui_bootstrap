<nav aria-label="breadcrumb">
  <ol class="breadcrumb">
    {% for item in items %}
      {% if item.url %}
        <li class="breadcrumb-item">
        <a href="{{ item.url }}">{{ item.text }}</a>
        </a>
      {% else %}
        <li class="breadcrumb-item active" aria-current="page">
        {{ item.text }}
      {% endif %}
    {% endfor %}
  </ol>
</nav>
